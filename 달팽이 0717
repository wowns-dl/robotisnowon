#include <stdio.h>
int main(){
	int a[5][5]={0,};
	int c=1;
	int n=5;
	for(int x=0; x<2; x++){
	    	for(int j=0; j<n-x; j++){
		a[0+x][j]=c++;
	}
	for(int i=0; i<5; i++){
		a[i][4]=c++;
	}
	for(int j=3; j>=0; j--){
	    a[4][j]=c++;
	   
	}
 for(int i=3; i>=1; i--){
     a[i][0]=c++; 
        }
		
	}
